<script type="text/javascript">
    //other maps
    var map;
    $(document).ready(function(){
      map = new GMaps({
        el: '#map',
        lat: -14.133983,
        lng: -47.516244,
		zoom:16,
		zoomControlOpt: {
        style : 'SMALL',
        position: 'TOP_LEFT'
    },
    panControl : false,
	mapTypeControl: false,
	zoomControl: true,
	streetViewControl: false,
	scrollwheel: false,
		
      });
	  
	  
$('#start_travel').click(function(e){
        e.preventDefault();
        map.travelRoute({
          origin: [-14.134961, -47.519656],
          destination: [-14.133812, -47.514736],
          travelMode: 'driving',
          step: function(e){
            $('#instructions').append('<li>'+e.instructions+'</li>');
            $('#instructions li:eq('+e.step_number+')').delay(850*e.step_number).fadeIn(500, function(){
              map.setCenter(e.end_location.lat(), e.end_location.lng());
              map.drawPolyline({
                path: e.path,
                strokeColor: '#66CD00',
                strokeOpacity: 0.6,
                strokeWeight: 20
              });
            });
          }
      });
	  });
	  
      map.addMarker({
        lat: -14.133905,
        lng: -47.514743,
        title: 'Eco Nois',
		icon: 'image/Econois_map_Cultural.png',
        infoWindow: {
          content: '<p>Espaço Cultural e Restaurante Eco Nois</p>'
        }
      });
	  
	  map.addMarker({
        lat: -14.133391,
        lng: -47.514716,
        title: 'Eco Nois',
		icon: 'image/Econois_map_Camping.png',
        infoWindow: {
          content: '<p>Camping, Hostel & Eco Chalets Eco Nois</p>'
        }
      });
	  
	  map.addMarker({
        lat: -14.133963,
        lng: -47.514987,
        title: 'Eco Nois',
		icon: 'image/Econois_map_BB.png',
        infoWindow: {
          content: '<p>Estamos ao lado do Banco do Brasil</p>'
        }
      });
	  
	  map.addMarker({
        lat: -14.134946,
        lng: -47.519727,
        title: 'Rodoviária',
		icon: 'image/Econois_map_Rodo.png',
        infoWindow: {
          content: '<p>Rodoviária de Alto Paraíso</p>'
        }
      });
	  
    });
    </script>
